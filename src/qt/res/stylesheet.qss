QToolBar{
/*    background-color:#FF4700;197F58*/
    /*background-color:#81c991;*/
    background-color:qconicalgradient(cx:1, cy:1, angle:0, stop:0.065 rgba(0, 250, 255, 255), stop:0.186441 rgba(20, 121, 255, 255), stop:0.327684 rgba(11, 227, 255, 255), stop:0.485876 rgba(28, 161, 255, 255), stop:0.655367 rgba(28, 216, 255, 255), stop:0.779661 rgba(28, 120, 255, 255), stop:0.949153 rgba(28, 197, 255, 255));

